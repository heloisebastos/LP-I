package br.edu.ifmt.sistemaacademico;
import util.Teclado;

public class Automoto {
	public static void main(String[] args) {
		char token = "i";
		//String lista[] = new String [10];
		String frase = Teclado.lerString("Digite a palavra");
		for (int i = 0; i < frase.length(); i++) {
			if (frase.charAt(i) == token) {
			       System.out.println("tudo certo e nada resolvido");
			       break;
			  }else {
				  System.out.println("deu ruim!!!:( ");
				  break;
			  }
		}
	}
}