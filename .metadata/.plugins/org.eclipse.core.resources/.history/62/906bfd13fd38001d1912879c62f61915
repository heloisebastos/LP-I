
public class MainPais {
	
	public static void main (String[] args) {
		Pais p1 = new Pais(codIso,nome,dimensao);
		String codIso = Teclado.lerString("Codigo ISO");
		String nome = Teclado.lerString("Nome pais");
		String dimensao = Teclado.lerString("Dimensao");
		
	    p1.Pais(codIso,nome,dimensao);
		
		System.out.println(p1.getCodIso());
	}	
}