package br.edu.ifmt.sistemaacademico;
import util.Teclado;

public class Automoto {
	public static void main(String[] args) {
		int key=0;
		//char lista[] = new char [10];
		String frase = Teclado.lerString("Digite a palavra");
		for (int j = 0; j < frase.length(); j++) {
			if (frase.charAt(j)== 'i') {
				    key=j;
				}
			}
		System.out.println(key);
	   }
	
}
